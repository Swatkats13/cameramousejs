{
  "name": "cameramouse",
  "productName": "cameramouse",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "sign": "electron-osx-sign out/cameramouse-darwin-arm64/cameramouse.app",
    "sign-python-excutable": "electron-osx-sign src/pyTracker/dist/main.app",
    "add-wincert-pass": "npm pkg set 'config.forge.makers[0].config.certificatePassword'=WINDOWS_CERTIFICATE_PASSWORD"
  },
  "keywords": [],
  "author": "mannysa",
  "license": "MIT",
  "files": [
    "**/*",
    "!src/pyTracker${/*}"
  ],
  "config": {
    "forge": {
      "all": true,
      "packagerConfig": {
        "icon": "src/assets/bu",
        "extraResource": [
          "src/pyTracker/dist"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "cameramouse",
            "authors": "Manny and Hao",
            "description": "Rewrite of cameramouse.org",
            "certificateFile": "certificate.pfx"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "connect-src 'self' https://cdn.socket.io/4.6.0/socket.io.min.js http: wss: ws:",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.65",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.7.1",
    "electron": "20.1.0",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.26.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.8",
    "@nut-tree/nut-js": "^2.2.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "http": "0.0.1-security",
    "http-errors": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "socket.io-client": "^4.6.2",
    "socketio": "^1.0.0",
    "url": "^0.11.0"
  }
}